pipeline{
    agent any
    stages{
        stage ("Build Docker Image"){
            steps{
                echo "Build "
               
            }
        }
        stage ("Run"){
            steps{
                echo "Run"
            }
        }
        
        stage("Deploy "){
            steps{
                echo "execute"
            }
        }
    }
    post{
        success{
            echo 'Pipeline completed scucessfull!'
        }
        failure{
            echo "Pipeline failed.Please check the logs."
        }
    }
}
